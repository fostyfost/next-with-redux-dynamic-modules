    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "6.0.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "redux-saga-test-plan": "4.0.1",
