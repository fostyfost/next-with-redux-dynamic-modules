    "redux-dynamic-modules-core": "5.2.3",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "redux-views": "1.0.1",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.8",
    "@types/nprogress": "0.2.0",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-redux": "7.1.11",
    "@types/redux": "3.6.0",
